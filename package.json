{
  "name": "ngx-plausible",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "format": "prettier --write \"src/**/*.{ts,scss,html}\" \"projects/**/*.{ts,scss,html}\" ",
    "build:lib": "ng build ngx-plausible && cp README.md dist/ngx-plausible",
    "publish:lib": "cd dist/ngx-plausible && npm publish",
    "rocket": "npm run build:lib && npm run publish:lib"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.0.2",
    "@angular/common": "^16.0.2",
    "@angular/compiler": "^16.0.2",
    "@angular/core": "^16.0.2",
    "@angular/forms": "^16.0.2",
    "@angular/platform-browser": "^16.0.2",
    "@angular/platform-browser-dynamic": "^16.0.2",
    "@angular/router": "^16.0.2",
    "rxjs": "~7.8.1",
    "tslib": "^2.5.2",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.0.2",
    "@angular/cli": "^16.0.2",
    "@angular/compiler-cli": "^16.0.2",
    "@types/node": "^16.11.7",
    "ng-packagr": "^16.0.1",
    "prettier": "2.8.8",
    "typescript": "~5.0.4"
  }
}