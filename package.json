{
  "name": "ngx-plausible",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "format": "prettier --write \"src/**/*.{ts,scss,html}\" \"projects/**/*.{ts,scss,html}\" ",
    "build:lib": "ng build ngx-plausible && cp README.md dist/ngx-plausible",
    "publish:lib": "cd dist/ngx-plausible && npm publish",
    "rocket": "npm run build:lib && npm run publish:lib"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.1",
    "@angular/common": "^14.2.1",
    "@angular/compiler": "^14.2.1",
    "@angular/core": "^14.2.1",
    "@angular/forms": "^14.2.1",
    "@angular/platform-browser": "^14.2.1",
    "@angular/platform-browser-dynamic": "^14.2.1",
    "@angular/router": "^14.2.1",
    "rxjs": "~7.5.4",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.2",
    "@angular/cli": "^14.2.2",
    "@angular/compiler-cli": "^14.2.1",
    "@types/node": "^12.20.0",
    "ng-packagr": "^14.2.1",
    "prettier": "2.5.1",
    "typescript": "~4.8.3"
  }
}