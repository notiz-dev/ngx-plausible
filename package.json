{
  "name": "ngx-plausible",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "format": "prettier --write \"src/**/*.{ts,scss,html}\"",
    "build:lib": "ng build ngx-plausible --configuration production && cp README.md dist/ngx-plausible",
    "publish:lib": "npm publish dist/ngx-plausible",
    "rocket": "npm run build:lib && npm run publish:lib"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.0",
    "@angular/common": "~13.0.0",
    "@angular/compiler": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/forms": "~13.0.0",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/router": "~13.0.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.1",
    "@angular/cli": "~13.0.1",
    "@angular/compiler-cli": "~13.0.0",
    "@types/node": "^12.20.0",
    "ng-packagr": "^13.0.3",
    "prettier": "2.4.1",
    "typescript": "~4.4.4"
  }
}
